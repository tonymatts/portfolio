 <CsoundSynthesizer>
<CsInstruments>
sr = 44100
kr = 4410
ksmps = 10
nchnls = 1
gar1 init 0
gar2 init 0
gar3 init 0
gad1 init 0

instr 1
idur = p3
ia = p4
ib = p5
klamp = p6
klcps = p7 ; slowed down by decrease
itype = p8;3 is good too
kamp = p9
kcps = p10
kcar = p11
kmod = p12
kndx = p13
ifn = p14
kfback = p15
  
	kres expon ia, idur, ib
	k1cps lfo klamp, kres, itype 
	adel line 0, p3*2, 0.01
	a1 foscil kamp, k1cps, kcar, kmod, kndx, ifn
	afl flanger a1, adel, kfback
 	out afl
	gar1 = gar1 + a1

endin


instr 2

kamp expseg 10000, p3/2, 10000, p3/2, 10000
ipitch = p4
iden = p5
iampoff = p6
ipitoff = p7
kgdur line p10, p3, p11
isft = p8
ienv = p9
islngth = ftlen(isft)
isftt = islngth/sr

	a1 grain kamp, ipitch, iden, iampoff, ipitoff, kgdur, isft, ienv, isftt
	out a1
	gar2 = gar2 + a1

endin


instr 3

idur = p3
kamp = p4
irise = p5
idec = p6
ia = p7
ib = p8
kbcps = p9
kbnh = p10
ifn = p11
	k1 linen kamp, irise, idur, idec
	k2 expon ia, idur, ib
	a1 buzz 1, cpspch(kbcps), k2+kbnh, ifn
	out k1*a1
	gar3 = gar3 + (k1 * a1)

endin
;--------------------------------------------------
instr 11
	a1 reverb gar1, 1.5 
	out a1
	garl = 0

endin



instr 21
	a1 reverb gar2, 1.5
	out a1
	gar2 = 0

endin

instr 31
	a1 reverb gar3, 1.5 
	out a1
	gar3 = 0

endin
</CsInstruments>
